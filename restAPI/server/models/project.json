{
  "name": "project",
  "base": "PersistedModel",
  "idInjection": true,
  "strict": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true
  },
  "mixins": {
    "TimeStamp" : true
  },
  "properties": {
    "authorId": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true,
      "index": true
    },
    "description": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "versions": {
      "type": "belongsTo",
      "model": "version",
      "foreignKey": "project_id"
    },
    "author": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "id"
    }
  },
  "acls": [],
  "methods": {}
}
